{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "ipfs-proxy",
	"main": "src/index.ts",
	"compatibility_date": "2025-01-24",
	"observability": {
		"enabled": true
	},
	"env": {
		"development": {
			"vars": {
				"APP_API_URL": "http://localhost:3000"
			}
		},
		"production": {
			"vars": {
				"APP_API_URL": "https://bgipfs.com"
			}
		}
	}
}
